  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>QHSEReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LocationCode">
            <Value>=Parameters!LocationCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@hazdDate">
            <Value>=Parameters!hazdDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  a.HazardID, a.HazardDate, a.SailHoc, a.Description, a.ConditionCode, a.SourceCode, a.ImmediateAction,  
                         a.PositionCode, a.CompanyID, a.RemedialCompleted, a.Status, a.LocationCode
FROM            Hazards AS a 
WHERE          ((a.LocationCode = @LocationCode) OR  (@LocationCode = 'All'))
And REPLACE(CONVERT(VARCHAR(11), a.HazardDate, 103),' ','-')   = @hazdDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
