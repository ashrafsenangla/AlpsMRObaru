    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EPRReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@HOCMonth">
            <Value>=Parameters!HOCMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@HOCYear">
            <Value>=Parameters!HOCYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@HOCLocationCode">
            <Value>=Parameters!HOCLocationCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select b.SeqNo, a.LocationCode, c.LocationName, 'HOC Analysis' Category, b.HazardCatName , count(*) TotalHOC 
From hazards a
LEFT JOIN HazardCategory b ON a.HazardCategoryCode = b.ID
Left JOIN QHSELocation c ON a.LocationCode = c.LocationCode
Where a.sailhoc = 'HOC'
And datename(month, a.hazarddate) = @HOCMonth
And datename(year, a.hazarddate) = @HOCYear
And a.LocationCode = @HOCLocationCode
And a.Status &lt;&gt; 'Draft'
And a.HazardCategoryCode = b.id 
Group by b.SeqNo, a.LocationCode, c.LocationName, b.HazardCatName
Order by 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
