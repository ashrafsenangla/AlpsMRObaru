                         HazardCategorySub AS c ON a.HazardCategorySubCode = c.ID INNER JOIN
                         HazardCategory AS b ON a.HazardCategoryCode = b.ID AND c.HazardCategoryCode = b.ID
WHERE        (a.SailHoc = 'HOC') AND (DATENAME(month, a.HazardDate) = @HOCMonth) AND (DATENAME(year, a.HazardDate) = @HOCYear) AND 
                         a.LocationCode = @HOCLocationCode AND (a.Status &lt;&gt; 'Draft') AND (b.SeqNo = 1)
