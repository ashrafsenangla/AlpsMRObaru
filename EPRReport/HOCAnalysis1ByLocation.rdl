    <DataSet Name="HOCRegister">
      <Query>
        <DataSourceName>EPRReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@HOCLocationCode">
            <Value>=Parameters!HOCLocationCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select b.LocationName, a.LocationCode, 
SUM(case when (ConditionCode = '4' OR ConditionCode = '5') then 1 else 0 end) as 'CountUAUC', 
SUM(case when (ConditionCode = '3' and Description = 'Safe') then 1 else 0 end) as 'CountSafe', 
SUM(case when IsTofs = 'true' then 1 else 0 end) as 'CountTOFS'
  From Hazards a
  LEFT JOIN QHSELocation b on a.LocationCode = b.LocationCode
  where datename(month, a.hazarddate) = 'April' AND a.LocationCode = @HOCLocationCode
  group by a.LocationCode, b.LocationName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
