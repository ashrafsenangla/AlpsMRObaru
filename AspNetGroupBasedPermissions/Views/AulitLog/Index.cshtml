@model IEnumerable<Core.Entities.Data.Admin.AuditLog>
@using AspNetGroupBasedPermissions.Helpers

@{
    ViewBag.Title = "Index";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                  <h5>List of AuditLog</h5>
				  <div class="ibox-tools">
					<div class="ibox-tools">
						@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                 </div>
				</div>
                <div class="ibox-content">

<table class="table table-striped table-bordered table-hover dataTables-example">
   <thead><tr>
        <th>
            @Html.DisplayNameFor(model => model.userid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eventdateutc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eventtype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tablename)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.recordid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.columnname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.originalvalue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.newvalue)
        </th>
        <th></th>
    </tr>
	</thead>
	<tbody>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eventdateutc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eventtype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tablename)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.recordid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.columnname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.originalvalue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.newvalue)
        </td>
        <td>
			@Html.EncodedActionLink("Details", "Details", "#routevalues",new { id=item.auditlogid }, new { @class = "btn-primary"})
            @Html.EncodedActionLink("Edit", "Edit","#routevalues", new { id=item.auditlogid }, new { @class = "btn-white"})
            @Html.EncodedActionLink("Delete", "Delete", "#routevalues", new { id=item.auditlogid }, new { @class = "btn-white"})
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>





@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
}

 @section Scripts {
            @Scripts.Render("~/plugins/dataTables")

  <script type="text/javascript">
        $(document).ready(function () {
            $('.dataTables-example').dataTable({
                "dom": 'T<"clear">lfrtip',
                "tableTools": {
                    "sSwfPath": "../scripts/plugins/dataTables/swf/copy_csv_xls_pdf.swf"
                }
            });
        });
    </script>
	}

