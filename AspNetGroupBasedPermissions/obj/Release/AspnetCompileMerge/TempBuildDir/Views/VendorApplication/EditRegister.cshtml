    function CompanyCodeChange(companyCode) {
        
        //alert(CompanyCode);
        if (companyCode == null || companyCode == "") {
            $('#CompanyVendors_PaymMode').html(""); // clear before appending new list
            $('#CompanyVendors_PaymMode').append(
                $('<option></option>').val("").html("-- Select Payment --"));
        }
        else {
            $.ajax({
                url: '@Url.Action("FillPaymentMode", "VendorApplication", new { area = "" })',
                type: "POST",
                dataType: "JSON",
                data: {
                    companyCode: companyCode,
                 
                },
                success: function (results) {
                    $('#CompanyVendors_PaymMode').html(""); // clear before appending new list
                    $('#CompanyVendors_PaymMode').append(
                $('<option></option>').val("").html("-- Select Payment --"));
                    $.each(results, function (i, results) {
                        $('#CompanyVendors_PaymMode').append(
                            $('<option></option>').val(results.key).html(results.value));
                    });
                },
                error: function (error) {
                    $.notify(
