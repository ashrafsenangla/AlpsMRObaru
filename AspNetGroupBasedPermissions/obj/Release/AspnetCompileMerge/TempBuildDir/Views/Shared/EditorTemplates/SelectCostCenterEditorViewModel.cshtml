@model AspNetGroupBasedPermissions.Models.SelectCostCenterEditorViewModel
@Html.HiddenFor(model => model.CostCenterCode)
<tr>
    <td style="text-align:center">
        @Html.CheckBoxFor(model => model.Selected)
    </td>

    <td><span></span></td>

    <td style="padding-right:20px">
        @Html.DisplayFor(model => model.CostCenterName)
    </td>

    <td style="padding-right:20px">
        @Html.CheckBoxFor(model => model.PurchaseOnBehalf)
    </td>

    <td style="padding-right:20px">
        @Html.CheckBoxFor(model => model.BulkPurchase)
    </td>
</tr>
