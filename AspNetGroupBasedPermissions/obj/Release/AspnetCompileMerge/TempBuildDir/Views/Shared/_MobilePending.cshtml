@model IEnumerable<Core.Entities.Data.Admin.Service>
@using AspNetGroupBasedPermissions.Helpers

<table style="padding-top:1px" class="table table-striped table-bordered table-hover dataTables-example">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameOption)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NameOption)
                </td>
            </tr>

        }
</table>
