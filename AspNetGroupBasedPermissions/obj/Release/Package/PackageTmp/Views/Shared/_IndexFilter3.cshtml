        <div class="dataTables_length pull-right">
            <label>
                <select class="form-control input-sm" style=" list-style:decimal; font-size:12px;" data-bind="value: pageSize">
                    <option value="10">10</option>
                    <option value="25">25</option>
                    <option value="50">50</option>
                    <option value="100">100</option>
                </select>
            </label>
        </div>


        @using (Html.BeginForm("IndexCCT", "CashCollectTransfer", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="ibox-tools pull-left" style="padding-top:5px;height:40px">
                <input type="file" name="file" />
            </div>
            <div class="ibox-tools pull-left" style="padding-top:5px;">
                <input type="submit" value="OK" />
            </div>

            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)
          
        }

        <div class="dataTables_filter">
            <label><input type="text" class="form-control input-sm pull-left" style="font-size:12px" data-bind="value: formVendorSearch, valueUpdate: 'keyup'" placeholder="Search">&nbsp;</label>
